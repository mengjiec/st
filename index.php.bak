<?php
error_reporting(E_ALL ^ E_NOTICE);// 显示除去 E_NOTICE 之外的所有错误信息
$qqlx = $_REQUEST['qqlx'];
if($qqlx=="h"){
$a='c.txt';
$b=file($a);
$c=count($b);
//$c="10000";
$anysh="background-color:#33A4A4;";
}else if($qqlx=="r"){
$a="b.txt";
$b=file($a);
$c=count($b);
//$c="10000";
$anysr="background-color:#33A4A4;";
}else{
$a="a.txt";
$b=file($a);
$c=count($b);
//$c="10000";
$anys="background-color:#33A4A4;";
}

$dg=mt_rand(0,$c);
$d=mt_rand(0,$dg);
$dc=mt_rand(0,2);
$dcj=mt_rand(-2,2);
$d+=$dcj;
$tpy=$b[$d];

//$setu = 'https://api.lolicon.app/setu/';
//$info=file_get_contents($setu);
preg_match('|z1z(.*?)z1z|i',$tpy,$zz);

preg_match('|b1t(.*?)b1t|i',$tpy,$bt);

preg_match('|p1id(.*?)p1id|i',$tpy,$pid);

preg_match('|u1id(.*?)ui1d|i',$tpy,$uid);

preg_match('|t1p(.*?)t1p|i',$tpy,$tpurl);


